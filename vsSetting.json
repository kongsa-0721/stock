{
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "liveServer.settings.port": 5000,
  "C_Cpp.updateChannel": "Insiders",
  "http.proxySupport": "off",
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.home": "/Library/Java/JavaVirtualMachines/jdk-15.0.1.jdk/Contents/Home",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },

  "editor.fontSize": 15,
  "editor.fontFamily": "Monaco",
  "git.autofetch": true,
  "terminal.integrated.tabs.enabled": true,
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "sonarlint.rules": {
    "javascript:S125": {
      "level": "off"
    },
    "Web:AvoidCommentedOutCodeCheck": {
      "level": "off"
    },
    "Web:ImgWithoutAltCheck": {
      "level": "off"
    },
    "Web:BoldAndItalicTagsCheck": {
      "level": "off"
    },
    "javascript:S3863": {
      "level": "off"
    },
    "javascript:S4144": {
      "level": "off"
    },
    "Web:TableWithoutCaptionCheck": {
      "level": "off"
    },
    "Web:S5256": {
      "level": "off"
    },
    "Web:TableHeaderHasIdOrScopeCheck": {
      "level": "off"
    },
    "javascript:S1117": {
      "level": "off"
    },
    "javascript:S4138": {
      "level": "off"
    },
    "javascript:S1186": {
      "level": "off"
    },
    "javascript:S1788": {
      "level": "off"
    },
    "typescript:S125": {
      "level": "off"
    },
    "typescript:S1186": {
      "level": "off"
    },
    "typescript:S1117": {
      "level": "off"
    },
    "typescript:S4325": {
      "level": "off"
    },
    "typescript:S905": {
      "level": "off"
    },
    "typescript:S878": {
      "level": "off"
    }
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveServer.settings.host": "localhost",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.triggerExpansionOnTab": true,
  "tabnine.experimentalAutoImports": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.detectIndentation": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.explorerKind": "external",
  "terminal.integrated.ignoreProcessNames": [
    "starship",
    "oh-my-posh",
    "bash",
    "zsh",
    "Tabby"
  ],
  "terminal.integrated.fontFamily": "Monaco",
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.shell.osx": "/bin/bash",
  "workbench.colorCustomizations": {
    //总的背景色
    "terminal.background": "#393b3d",
    //local颜色
    "terminal.foreground": "#ff5fbc",
    //端口号 文件夹名字
    "terminal.ansiBrightCyan": "#f8bbbb",
    //分支颜色
    "terminal.ansiBrightGreen": "#1eff00",
    //名字
    "terminal.ansiBrightYellow": "#fffb00",
    //vite颜色
    "terminal.ansiCyan": "#a9f7ec",
    //running颜色
    "terminal.ansiGreen": "#64ff78",
    "terminalCursor.background": "#D8D8D8",
    "terminalCursor.foreground": "#D8D8D8",
    "terminal.ansiBlack": "#181818",
    "terminal.ansiBlue": "#d36464",
    "terminal.ansiBrightBlack": "#bea8a8",
    "terminal.ansiBrightBlue": "#ad3b3b",
    "terminal.ansiBrightMagenta": "#943d3d",
    "terminal.ansiBrightRed": "#db3a35",
    "terminal.ansiBrightWhite": "#F8F8F8",
    "terminal.ansiMagenta": "#bd9f9f",
    "terminal.ansiRed": "#AB4642",
    "terminal.ansiWhite": "#31962e",
    "terminal.ansiYellow": "#e7da22",
    "terminal.integrated.letterSpacing": 0.1,
    "terminal.integrated.fontSize": 30 //字体大小设置
  },
  "terminal.external.osxExec": "iterm.app",
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "workbench.iconTheme": "material-icon-theme",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.printWidth": 90,
  "vim.normalModeKeyBindings": [
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["g", "_"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.operatorPendingModeKeyBindings": [
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["g", "_"]
    }
  ]
}
